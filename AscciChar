import  java.util.Scanner;

public class Main {

    public static void main(String[] args) {
	Scanner scanner=new Scanner(System.in);
    char ch =scanner.next().charAt(0);
    int asci=(char)ch;
    if(asci>97){
      System.out.print((char)(asci-32));
    }else{
       System.out.print((char)(asci+32));
    }
    }
}
