import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
	Scanner scanner=new Scanner(System.in);
    int first=scanner.nextInt();
    int second=scanner.nextInt();
    int year=1;
    for(int i=0;first<second;i++){
        first=first*3;
        second=second*2;
        year++;
    }
    System.out.println(year);

    }
}
